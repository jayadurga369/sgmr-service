{
    "name": "sgmr-service",
    "version": "0.1.0",
    "description": "A frontend service for interacting with the sGMR Data API",
    "scripts": {
        "build": "webpack --mode=production",
        "build:dev": "webpack --mode=development",
        "lint": "eslint -f table",
        "lint:extra": "eslint \"./src/**/*.{js,jsx}\" --fix",
        "start": "webpack serve --mode=development",
        "test": "jest",
        "test:coverage": "jest --coverage --colors",
        "test:watch": "npm run test -- --watch",
        "cypress:test:local": "cypress run",
        "cypress:test:dev": "cypress run -- --env configFile=dev",
        "cypress:runner": "cypress open",
        "cypress:test:report": "node cypress_runner"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:UKHomeOffice/sgmr-service.git"
    },
    "author": "Pedro Miguel",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/UKHomeOffice/sgmr-service/issues"
    },
    "homepage": "https://github.com/UKHomeOffice/sgmr-service#readme",
    "jest": {
        "moduleNameMapper": {
            "^config$": "<rootDir>/src/lib/config.js",
            "^@components(.*)$": "<rootDir>/src/components$1",
            "^@constants(.*)$": "<rootDir>/src/constants$1",
            "^@lib(.*)$": "<rootDir>/src/lib$1",
            "^@state(.*)$": "<rootDir>/src/state$1",
            "^@utils(.*)$": "<rootDir>/src/utils$1"
        },
        "setupFilesAfterEnv": [
            "<rootDir>/src/setupTests.js"
        ],
        "modulePathIgnorePatterns": [
            "cypress"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.8.4",
        "@babel/node": "^7.8.4",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.12.1",
        "@babel/preset-env": "^7.8.4",
        "@babel/preset-react": "^7.8.3",
        "@testing-library/dom": "^7.24.5",
        "@testing-library/jest-dom": "^5.11.4",
        "@testing-library/react": "^12.1.2",
        "@testing-library/user-event": "^12.1.7",
        "@types/faker": "^5.1.3",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.0.6",
        "copy-webpack-plugin": "^10.2.0",
        "cypress": "^5.2.0",
        "cypress-axe": "^0.10.0",
        "cypress-get-table": "^1.0.1",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-plugin-babel": "^5.3.0",
        "eslint-plugin-cypress": "^2.11.1",
        "eslint-plugin-filenames": "^1.3.2",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-jest": "^22.15.1",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.18.3",
        "eslint-plugin-react-hooks": "^1.7.0",
        "faker": "^5.1.0",
        "file-loader": "^5.1.0",
        "fs-extra": "^9.0.1",
        "html-webpack-plugin": "^4.5.0",
        "jest": "^27.4.5",
        "ls": "^0.2.1",
        "mailslurp-client": "^10.0.4",
        "mocha": "^8.2.1",
        "mochawesome": "^6.2.1",
        "mochawesome-merge": "^4.2.0",
        "mochawesome-report-generator": "^5.1.0",
        "msw": "^0.21.2",
        "node-sass": "^4.14.1",
        "prettier": "^1.19.1",
        "rimraf": "^3.0.2",
        "sass-loader": "^8.0.2",
        "webpack": "^5.65.0",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^4.7.0",
        "yargs": "^16.1.1"
    },
    "dependencies": {
        "@babel/runtime-corejs3": "^7.9.2",
        "accessible-autocomplete": "^2.0.3",
        "axios": "^0.21.2",
        "browserslist": "^4.16.5",
        "dayjs": "^1.10.7",
        "govuk-frontend": "^3.5.0",
        "history": "^4.10.1",
        "lodash.debounce": "^4.0.8",
        "prop-types": "^15.7.2",
        "qs": "^6.9.4",
        "react": "^16.12.0",
        "react-app-polyfill": "^1.0.6",
        "react-dom": "^16.12.0",
        "react-router-dom": "^5.1.2",
        "regenerator-runtime": "^0.13.7"
    }
}
